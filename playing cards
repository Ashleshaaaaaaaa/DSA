#include <iostream>
using namespace std;

int main() {
    int i, j, n = 5;
    int key;
    int cards[5];

    cout << "Enter " << n << " cards:" << endl;
    for (i = 0; i < n; i++) {
        cin >> cards[i];
    }

    // Insertion Sort
    for (i = 1; i < n; i++) {
        key = cards[i];
        j = i - 1;

        while (j >= 0 && cards[j] > key) {
            cards[j + 1] = cards[j];
            j--;
        }
        cards[j + 1] = key;
    }

    cout << "Sorted cards: ";
    for (i = 0; i < n; i++) {
        cout << cards[i] << " ";
    }

    return 0;
}

